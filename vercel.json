
{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.ts": {
      "memory": 3008,
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
